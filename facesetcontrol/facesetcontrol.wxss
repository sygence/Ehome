@import '../pages/form/form.wxss';
.scanner {
  margin-top: 5%;
  display: flex;
  overflow: hidden;
  flex-direction: column;
  margin-left: 35%;
  height: 280rpx;
  width: 30%;
  border: 1px solid #000;
  background-color: #fff;
  border-top: 1rpx solid #D9D9D9;
  border-bottom: 1rpx solid #D9D9D9;
  align-items: center;
  justify-content: center;
}

.chooseImage{
  overflow: hidden;
  margin-bottom: 30%;
  z-index: 1;
}
.image-plus-text{
  overflow: hidden;
  text-align: center;
}
.page-section{
  overflow: hidden;
  margin-bottom: 20rpx;
}
.py-input{
  justify-content: center;
  text-align: center;
  margin-top: 10px;
  margin-left: 25%;
  border-top: 1px solid black;
  border-bottom: 1px solid black;
  width: 60%;
}
.scanner .canvas {
  overflow: hidden;
  flex: 1;
  width: 100%;
}
.text-view{

  margin-top: -100rpx;
}
.canvas{
  overflow: hidden;
  position: absolute;
  z-index: 10
}
.image-plus {
  width: 150rpx;
  height: 200rpx;
  border: 2rpx solid #D9D9D9;
  position: relative;
}
.image-plus-nb{
  border: 0;
}
.image-plus-text{
  color: #888888;
  height: 30rpx;
}
.image-plus-horizontal {
  position: absolute;
  top: 50%;
  left: 50%;
  background-color: #d9d9d9;
  width: 4rpx;
  height: 80rpx;
  transform: translate(-50%, -50%);
}
.image-plus-vertical {
  position: absolute;
  top: 50%;
  left: 50%;
  background-color: #d9d9d9;
  width: 80rpx;
  height: 4rpx;
  transform: translate(-50%, -50%);
}

.scanner .buttons {
  display: flex;
  width: 100%;
  height: 120rpx;
  color: #4f6bf0;
  line-height: 120rpx;
  background-color: black;
  justify-content: space-around;
  

}
.buttons{
  border: 1px solid antiquewhite;
  width: 80rpx;
  height: 60rpx;
  background-color: burlywood; 
  margin: 0px auto;
  border-radius:15px;
}
.btn{
  width: 100%;
  height: 100%;
  display: flex;
  justify-content:center;
  padding-top: 10rpx;

}

.buttons .choose, .buttons .confirm {
  flex: 1;
  text-align: center;
  margin-top: 20%;
}

